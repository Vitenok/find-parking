<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	metadata-complete="true" version="3.0">
	<display-name>Parking Places Around The City</display-name>
	<session-config>
  <session-timeout>1</session-timeout> 
</session-config>
	<servlet>
		<servlet-name>Parking Viewer</servlet-name>
		<servlet-class>com.iti.parking.controller.AllPlacesController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Parking Viewer</servlet-name>
		<url-pattern>/AllPlacesController.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Parking Viewer Admin</servlet-name>
		<servlet-class>com.iti.parking.controller.AdminController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Parking Viewer Admin</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Parking Viewer Client</servlet-name>
		<servlet-class>com.iti.parking.controller.ClientController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Parking Viewer Client</servlet-name>
		<url-pattern>/client</url-pattern>
	</servlet-mapping>
</web-app>